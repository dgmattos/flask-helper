{
  "name": "flask-helper",
  "displayName": "Flask RUN APP",
  "publisher": "maveric",
  "description": "Extens√£o para automatizar ambiente Flask no VSCode",
  "version": "1.0.4",
  "engines": {
    "vscode": "^1.50.0"
  },
  "activationEvents": [
    "onView:flaskRunAppView"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "flaskRunApp",
          "title": "Flask RUN APP",
          "icon": "media/icon.png"
        }
      ]
    },
    "views": {
      "flaskRunApp": [
        {
          "id": "flaskRunAppView",
          "name": "Comandos Flask"
        }
      ]
    },
    "commands": [
      {
        "command": "flaskRunApp.createVenv",
        "title": "Criar venv"
      },
      {
        "command": "flaskRunApp.installReqs",
        "title": "Instalar requirements.txt"
      },
      {
        "command": "flaskRunApp.runFlask",
        "title": "Rodar Flask App"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "@types/vscode": "^1.50.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}
